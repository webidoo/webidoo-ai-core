{
	"name": "@webidoo-eng/webidoo-ai-core",
	"version": "1.0.0",
	"description": "AI core library with OpenAI wrapper and Redis vector store for inference and RAG applications",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc && tsc-alias",
		"type-check": "tsc --noEmit",
		"prepublishOnly": "npm run build",
		"test": "npm run test:unit && npm run test:integration",
		"test:unit": "vitest run --config vitest.config.ts",
		"test:integration": "vitest run --config vitest.integration.config.ts"
	},
	"keywords": [
		"ai",
		"openai",
		"vector-store",
		"redis",
		"rag",
		"inference",
		"typescript",
		"embeddings"
	],
	"author": "Webidoo",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/webidoo/webidoo-ai-core.git"
	},
	"bugs": {
		"url": "https://github.com/webidoo/webidoo-ai-core/issues"
	},
	"homepage": "https://github.com/webidoo/webidoo-ai-core#readme",
	"engines": {
		"node": ">=16.0.0"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/node": "^24.1.0",
		"tsc-alias": "^1.8.10",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"mammoth": "^1.9.1",
		"openai": "^5.10.2",
		"redis": "^5.6.0"
	}
}